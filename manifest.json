{
  "manifest_version": 3,
  "name": "Aki Home",
  "description": "A simple home page aiming to stand out",
  "version": "1.0",
  "icons": {
    "16": "assets/aki16.png",
    "48": "assets/aki48.png",
    "128": "assets/aki128.png",
    "256": "assets/aki256.png"
  },
  "action": {
    "default_title": "Aki Home",
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "assets/aki16.png",
      "48": "assets/aki48.png",
      "128": "assets/aki128.png",
      "256": "assets/aki256.png"
    }
  },
  "permissions": ["tabs", "bookmarks"],
  "chrome_url_overrides": {
    "newtab": "aki/aki.html"
  }
}
